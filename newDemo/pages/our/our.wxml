<!--pages/our/our.wxml-->

<view class="beforeTime">
	<text  selectable space="ensp">大家伙的Demo :
	</text>
</view>
<view class="time">
	<text space="ensp">第 {{days}} 天
	</text>
</view>

<view class="rate">
	<van-rate
	value="{{ value }}"
	size="{{ 25 }}"
	color="#ffd21e"
	void-icon="star"
	void-color="#eee"
	bind:change="onChange"
	/>
</view>




